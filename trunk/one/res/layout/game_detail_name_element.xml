<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
			    android:layout_width="320px"
			    android:layout_height="25px"
			    android:background="#16329e">	
			    <TableRow	android:layout_width="320px"
							android:layout_height="15px">
					<TextView android:id="@+id/guest_name"
						android:layout_width="130px"
						android:layout_height="18px"
						android:layout_gravity="center_vertical|right"
			    		android:textColor="#ffffff"
			    		android:textStyle="bold"
			    		android:textSize="14sp"
			    		android:text="guest scorer"
			    		></TextView>
			    	<TextView android:id="@+id/game_detail_blank1"
						android:layout_width="60px"
						android:layout_height="18px"
			    		android:text=""
			    		></TextView>
			    	<TextView android:id="@+id/home_name"
						android:layout_width="130px"
						android:layout_height="18px"
						android:layout_gravity="center_vertical|left"
			    		android:textColor="#ffffff"
			    		android:textStyle="bold"
			    		android:textSize="14sp"
			    		android:text="home scorer"
			    		></TextView>
		    	</TableRow>
</TableLayout>