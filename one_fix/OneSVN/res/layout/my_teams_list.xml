<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="#ffffff">

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="320px" android:layout_height="47px"
			android:background="@drawable/head_bar">

			<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:orientation="horizontal" android:layout_width="fill_parent"
				android:gravity="center_vertical" android:layout_height="wrap_content"
				android:background="@drawable/head_bar">

				<ImageButton android:id="@+id/my_teams_back"
					android:layout_width="38px" android:layout_height="38px"
					android:layout_marginLeft="2dip" android:layout_alignParentTop="true"
					android:drawSelectorOnTop="true" android:layout_alignParentLeft="true"
					android:src="@drawable/blank" android:background="@drawable/my_teams_done_btn_rollover" />

				<ImageButton android:id="@+id/my_teams_add"
					android:layout_width="38px" android:layout_height="38px"
					android:layout_marginRight="2dip" android:layout_alignParentTop="true"
					android:drawSelectorOnTop="true" android:layout_alignParentRight="true"
					android:src="@drawable/blank" android:background="@drawable/my_teams_add_button" />
			</RelativeLayout>

		</FrameLayout>
		<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="320px" android:layout_height="25px">
			<!--
				<ImageView android:id="@+id/navi_bar" android:layout_width="320px"
			-->
			<!--
				android:layout_height="25px" android:src="@drawable/navigation_bar"
				/>
			-->
			<TextView android:id="@+id/navi_bar" android:layout_width="320px"
				android:gravity="center_horizontal" android:layout_height="25px"
				android:background="@drawable/my_teams_element" android:textColor="@color/WHITE" />
		</FrameLayout>
		<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="320px" android:layout_height="336px">
			<ListView android:id="@+id/android:list"
				android:layout_width="fill_parent" android:layout_height="wrap_content" />
			<ProgressBar android:id="@+id/progressbar"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_gravity="center" />
		</FrameLayout>
	</LinearLayout>



	<!-- ################ [ Start banner ] ################ -->
	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_gravity="center_horizontal|bottom">
		<ImageView android:layout_width="fill_parent" android:id="@+id/bannerIView"
			android:layout_marginBottom="0dip" android:layout_alignParentBottom="true"
			android:background="@drawable/banner_bg" android:layout_height="wrap_content">
		</ImageView>
	</RelativeLayout>
	<!-- ################ [ End banner ] ################ -->

</FrameLayout>